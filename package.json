{
  "name": "veiviserbygger",
  "version": "3.2.0",
  "description": "Wizard builder for Losen ===",
  "main": "index.js",
  "scripts": {
    "start": "turbo start",
    "build": "turbo build --filter=${BUILD_FILTER:-*}",
    "lint": "turbo lint",
    "dev": "turbo dev",
    "test": "turbo test",
    "test:watch": "turbo test -- --watch",
    "build:docker": "docker build -f Dockerfile . -t losen-veiviserbygger",
    "prepare": "husky",
    "release:patch": "npm version patch -m \"chore(release): %s\" && git push --follow-tags",
    "release:minor": "npm version minor -m \"chore(release): %s\" && git push --follow-tags",
    "release:major": "npm version major -m \"chore(release): %s\" && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DirektoratetForByggkvalitet/veiviserbygger.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DirektoratetForByggkvalitet/veiviserbygger/issues"
  },
  "homepage": "https://github.com/DirektoratetForByggkvalitet/veiviserbygger#readme",
  "volta": {
    "node": "20.19.4",
    "npm": "10.9.3"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "turbo": "^2.1.3",
    "wait-on": "^8.0.1"
  },
  "packageManager": "npm@10.0.0",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "losen": "^10.0.2"
  }
}
